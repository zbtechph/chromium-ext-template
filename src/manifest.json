{
    "name": "ZFlix",
    "version": "0.1.0",
    "manifest_version": 3,
    "description": "Yet another cookie editor.",
    "permissions": [
        "cookies",
        "storage"
    ],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup/index.html",
        "default_icon": {
            "16": "assets/favicon-16x16.png",
            "32": "assets/favicon-32x32.png"
        }
    },
    "options_ui": {
        "page": "options/index.html",
        "open_in_tab": true
    }
}